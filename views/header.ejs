    <style>
      .temp_div {
        border: 2px solid black;
        border-radius: 30px;
        margin-top: 20px;
        margin-bottom: 20px;
        padding: 20px;
      }
      .nav-temp {
        padding: 20px;
        margin-left: auto;
        margin-right: auto;
      }
      .nav-title {
        font-size: 2rem;
        font-weight: 500;
        color: white;
      }
      .page-title {
        font-size: x-large;
        font-family: "Lucida Sans", "Lucida Sans Regular", "Lucida Grande",
          "Lucida Sans Unicode", Geneva, Verdana, sans-serif;
        font-weight: 600;
        background-color: #04aa6d;
        color: aliceblue;
        padding: 10px;
        border-radius: 10px;
      }
      .stand-margin {
        margin-top: 20px;
      }
      h3 {
        font-size: 18px;
        padding: 0%;
        font-family: "Lucida Sans", "Lucida Sans Regular", "Lucida Grande",
          "Lucida Sans Unicode", Geneva, Verdana, sans-serif;
        font-weight: 600;
        color: seagreen;
      }
      input[type="text"],
      input[type="email"],
      input[type="number"] {
        border: 2px solid #212529;
      }
      label {
        color: black;
        font-size: large;
        font-weight: 500;
      }

      a {
        font-size: 14px;
        padding: 10px;
        outline: none;
        text-decoration: none;
        padding: 2px 1px 0;
      }
      footer {
        text-align: center;
        padding: 3px;
        background-color: rgb(194, 188, 186);
        color: rgb(0, 0, 0);
      }
      .info-data {
        font-size: 12px;
        color: seagreen;
      }
      h4 {
        margin: 10px;
        font-size: medium;
        color: #04aa6d;
      }
      .note-info {
        font-size: 15px;
        color: #333;
      }
      /* navbar */
      ul {
        list-style-type: none;
        margin: 0;
        padding: 0;
        overflow: hidden;
        background-color: #333;
      }

      li {
        float: left;
      }

      li a {
        display: block;
        color: white;
        text-align: center;
        padding: 14px 16px;
        text-decoration: none;
      }

      li a:hover:not(.active) {
        background-color: #111;
      }

      .active {
        background-color: #04aa6d;
      }
